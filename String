#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
    string a,b;
    cin>>a;
    cin>>b;
    cout<<a.length()<<" "<<b.length()<<endl;
    cout<<a+b<<endl;
    char c,d;
    c =b[0];
    d =a[0];
    a[0]=c;
    b[0] =d;
    cout<<a<<" "<<b<<endl;
    
        
        
  
    
  
    return 0;
}
